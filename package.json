{
  "name": "boilerplate-mern",
  "version": "1.0.0",
  "description": "boilerplate for mern applications",
  "main": "server/server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon -r dotenv/config server/server.js",
    "build": "cd client && npm run build",
    "start:client": "cd client && npm start",
    "clear": "rm -rf server/public .parcel-cache"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airgead73/boilerplate-mern.git"
  },
  "keywords": [
    "parcel",
    "mongodb",
    "express",
    "react",
    "node",
    "boilerplate"
  ],
  "author": "dan moneypenny",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airgead73/boilerplate-mern/issues"
  },
  "homepage": "https://github.com/airgead73/boilerplate-mern#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "mongoose": "^6.2.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.15"
  }
}
